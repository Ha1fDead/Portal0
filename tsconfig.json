{
	"compilerOptions": {
		"allowJs": false,
		"forceConsistentCasingInFileNames": true,
		//"module": This is local to the config
		"moduleResolution": "node",
		"noImplicitAny": true,
		//"outDir": This is local to /config/tsconfig and /server/tsconfig
		"preserveConstEnums": true,
		"pretty": true,
		"removeComments": true,
		"sourceMap": true,
		"strict": true,
		"target": "esnext",
	},
	// local tsconfig specifies local paths
	//"include": [
	//	"./**/*"
	//]
}
